{
    "manifest_version": 2,
    "name":"Preview Page Plugin",
    "version":"2.0.0",
    "description":"Данный плагин позволяет быстро посмотреть содержимое веб страницы не посещая сам сайт.",
    "icons": {"128": "/icons/icon_128.png"},
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "browser_action": {
        "default_icon": "/icons/icon.png"
    },

    "permissions": [
        "activeTab"
    ]
}